{
  "name": "npm-pew-pew",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "packageManager": "yarn@4.6.0",
  "description": "Pewpew Vue 3 Component Library based on Naive UI. Uses TypeScript.",
  "keywords": [
    "pewpew",
    "component library",
    "ui",
    "ui framework",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mzymta/npm-pew-pew"
  },
  "author": "mzymta",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vite build && vue-tsc",
    "build-only": "vite build",
    "test:unit": "vitest",
    "lint:types": "vue-tsc --build --force",
    "lint:eslint": "eslint --no-fix --max-warnings 0",
    "lint:oxlint": "oxlint . -D correctness",
    "lint:stylelint": "stylelint \"src/**/*.{vue,css,scss}\"",
    "fix:eslint": "eslint . --fix",
    "fix:oxlint": "oxlint . --fix -D correctness",
    "fix:stylelint": "stylelint \"src/**/*.{vue,css,scss}\" --fix",
    "fix:prettier": "prettier --write \"{src,.storybook}/\"",
    "lint": "run-s \"lint:*\"",
    "fix": "run-s \"fix:*\"",
    "stylelint-scss-check": "stylelint-config-prettier-scss-check",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "js-cookie": "^3.0.5",
    "naive-ui": "^2.40.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.8.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/manager-api": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/theming": "^8.2.9",
    "@storybook/vue3": "^8.2.9",
    "@storybook/vue3-vite": "^8.2.9",
    "@tsconfig/node20": "^20.1.4",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.14.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-oxlint": "^0.11.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.30.0",
    "husky": "^9.1.6",
    "js-cookie": "^3.0.5",
    "jsdom": "^25.0.1",
    "naive-ui": "^2.40.1",
    "npm-run-all2": "^7.0.1",
    "oxlint": "^0.11.0",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.1",
    "sass-embedded": "^1.80.6",
    "semantic-release": "^24.2.1",
    "storybook": "^8.2.9",
    "stylelint": "^16.10.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.8.1",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^3.5.1",
    "vite-plugin-lib-inject-css": "^2.1.0",
    "vite-plugin-static-copy": "^0.17.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vitest": "^2.1.4",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue-tsc": "^v2.1.10"
  },
  "type": "module",
  "module": "./dist/npm-pew-pew.es.js",
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/npm-pew-pew.es.js"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    },
    "./src/": {
      "import": "./src/",
      "require": "./src/"
    }
  }
}
